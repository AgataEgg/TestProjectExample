<Window x:Class="Турфирма_Притужалова_Д199.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Турфирма_Притужалова_Д199"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="76*"/>
            <RowDefinition Height="303*"/>
            <RowDefinition Height="40*"/>
        </Grid.RowDefinitions>
        <Grid Background="#bae3e8"/>
        <Image Source="Resources/логотип.png" HorizontalAlignment="Left" Width="164"/>
        <TextBlock Text="header" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Height="40" Margin="350,22,350,14" Width="92"></TextBlock>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="back" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"></Button>
       
    </Grid>
</Window>
